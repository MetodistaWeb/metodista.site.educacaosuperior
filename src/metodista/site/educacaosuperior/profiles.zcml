<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    xmlns:gs="http://namespaces.zope.org/genericsetup"
    i18n_domain="metodista.site.educacaosuperior">

    <!-- Profiles -->
    <gs:registerProfile
        name="default"
        title="Portal de Educação Superior: Instalação Inicial"
        directory="profiles/default"
        description="Instalação e configuração de Portal de Educação Superior"
        provides="Products.GenericSetup.interfaces.EXTENSION" />

    <gs:registerProfile
        name="uninstall"
        title="Portal de Educação Superior: Remover"
        directory="profiles/uninstall"
        description="Remove configurações de Portal de Educação Superior"
        provides="Products.GenericSetup.interfaces.EXTENSION" />


</configure>
